{% extends "layout.njk"%}

{% block head %}
  <title>About</title>
{% endblock %}

{% block content %}

  <div id="wrapper">
    <img src="{{ about.avatar_url }}" alt="Daniel de Paula">
    <h1>{{ about.name }}</h1>
    <h2>{{ about.role }}</h2>
    <p>{{ about.description }}</a> </p>
  </div>
  <div id="links-footer" class="links">
    {% for link in about.links %}
    <a href="{{ link.url }}" target="_blank">{{ link.name }}</a>
    {% endfor %} 
  </div>
{% endblock %}

